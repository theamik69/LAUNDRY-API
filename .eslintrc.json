{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "strict": 0,
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "no-console": 0,
        "consistent-return": 0,
        "no-plusplus": 0,
        "func-names": 0,
        "prefer-destructuring": 0
    }
}
